{
  "metadata": [
    {
      "src": [
        {
          "files": ["Ariadne.dll"],
          "cwd": "bin/Release/net8.0"
        }
      ],
      "dest": "api",
      "filter": "apiFilter.yml"
    }
  ],
  "build": {
    "content": [
      { "files": ["index.md", "toc.yml"] },
      { "files": ["docs/**/*.md"], "exclude": ["**/obj/**"] },
      { "files": ["api/**/*.yml", "api/**/*.md"], "exclude": ["**/obj/**"] }
    ],
    "resource": [
      { "files": ["docs/assets/**"] }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Ariadne",
      "_appName": "Ariadne",
      "_description": "Grasshopper plugin for inverse design of form-found structures using the force density method."
    },
    "fileMetadata": {},
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}
