{
  "metadata": [
    {
      "src": [
        {
          "files": ["bin/Release/net8.0/Ariadne.dll"]
        }
      ],
      "dest": "api",
      "filter": "apiFilter.yml"
    }
  ],
  "build": {
    "content": [
      { "files": ["index.md", "toc.yml"] },
      { "files": ["api/**/*.yml", "api/**/*.md"], "exclude": ["**/obj/**"] }
    ],
    "resource": [],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Ariadne API Reference",
      "_appName": "Ariadne",
      "_description": "API reference for the Ariadne Grasshopper plugin."
    },
    "fileMetadata": {},
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}
